<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.blue.jk.mapper.ExportProductMapper">
    <!-- 开启本mapper的缓存  mybatis默认实现PerpetualCache 假如要和 ehcache实现类 cache type 修改为此 -->
    <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>

    <resultMap id="factoryRM" type="cn.blue.jk.domain.ExportProduct">
        <id column="EXPORT_PRODUCT_ID" property="id"/>
        <result column="FULL_NAME" property="fullName"/>
        <result column="FACTORY_NAME" property="factoryName"/>
        <result column="PHONE" property="phone"/>
        <result column="MOBILE" property="mobile"/>
        <result column="FAX" property="fax"/>
        <result column="CNOTE" property="cnote"/>
        <result column="INSPECTOR" property="inspector"/>
        <result column="STATE" property="state"/>

        <result column="ORDER_NO" property="orderNo"/>
        <result column="CREATE_BY" property="createBy"/>
        <result column="CREATE_DEPT" property="createDept"/>
        <result column="CREATE_TIME" property="createTime"/>
    </resultMap>
    <select id="find" parameterType="map" resultMap="factoryRM">
        SELECT * from export_product_c where 1=1
        <if test="state != null">and STATE=#{state}</if>
    </select>

    <select id="get" parameterType="java.io.Serializable" resultMap="factoryRM" useCache="true" flushCache="true">
        SELECT * from export_product_c where EXPORT_PRODUCT_ID=#{id}
    </select>

    <insert id="insertFactory" parameterType="cn.blue.jk.domain.ExportProduct">
        insert into export_product_c(EXPORT_PRODUCT_ID,FULL_NAME,FACTORY_NAME,PHONE,MOBILE,FAX,CNOTE,INSPECTOR,STATE,ORDER_NO,CREATE_BY,CREATE_DEPT,CREATE_TIME) values(
        #{id},#{fullName},#{factoryName},#{phone},#{mobile},#{fax},#{cnote},#{inspector},#{state},#{orderNo},#{createBy},#{createDept},#{createTime})
    </insert>

    <update id="update" parameterType="cn.blue.jk.domain.ExportProduct">
        update export_product_c
        <set>

        </set>
        where EXPORT_PRODUCT_ID=#{id}
    </update>



    <delete id="deleteById" parameterType="string">
        delete from export_product_c where EXPORT_PRODUCT_ID=#{id}
    </delete>

    <delete id="delete" parameterType="string">
        delete from export_product_c WHERE
         <foreach collection="ids" item="item_id" open="EXPORT_PRODUCT_ID in(" close=")" separator=",">
            #{item_id}
         </foreach>
    </delete>


</mapper>